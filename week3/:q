#include <stdio.h>


int id = 0;
int generate_id(){
	return ++id;
}


struct node {
	int data; 
	struct node* next;
	int id;
};

struct linked_list {
	node head;
	int node_count;
}


void print_list(struct linked_list  *list){
	struct node temp = list -> head;
	for (int i=0; i<list->node_count; i++) {
		printf("id: %d, data: %d", temp.id, temp.data);
		temp = temp.next;
	}
}

void insert_node (struct linked_list* list, struct node* new, int id_of_prev){
	struct node* prev
	struct node temp = list -> head;
	while (temp != *prev)
		temp = temp.next;
	
	new -> next = prev -> next;
	prev -> next = new;
}


void delete_node(struct linked_list* list, struct node* deleting){
	
	struct node temp = list -> head;
	while (temp.next != *deleting)
		temp = temp.next;
	
	temp.next = deleting -> next;
}



int main(){
	printf("The linked list is empty now.\n
			Press \"1\" to add elements,\n
			press \"2\" to delete elements,\n,
			press \"3\" to print contents)
	while 
}
